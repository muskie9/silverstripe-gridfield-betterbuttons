{
    "name": "unclecheese/betterbuttons",
    "description": "Adds new form actions and buttons to GridField detail form for usability enhancements.",
    "type": "silverstripe-module",
    "keywords": ["silverstripe", "gridfield", "buttons"],
    "license": "GNU",
    "authors": [
        {
            "name": "Uncle Cheese",
            "email": "unclecheese@leftandmain.com"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.0@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "javascript",
            "css"
        ]
    },
    "autoload": {
        "psr-4": {
            "UncleCheese\\BetterButtons\\": "src/",
            "UncleCheese\\BetterButtons\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
